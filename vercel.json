{
  "redirects": [
    { "source": "/(.*)", "destination": "https://data-blast.com/$1", "has": [{ "type": "host", "value": "www.data-blast.com" }], "permanent": true },
    { "source": "/(.*)", "destination": "https://gamer-guide.com/$1", "has": [{ "type": "host", "value": "www.gamer-guide.com" }], "permanent": true },
    { "source": "/(.*)", "destination": "https://hair-guru.com/$1", "has": [{ "type": "host", "value": "www.hair-guru.com" }], "permanent": true },
    { "source": "/(.*)", "destination": "https://vr-live.com/$1", "has": [{ "type": "host", "value": "www.vr-live.com" }], "permanent": true },
    { "source": "/(.*)", "destination": "https://xr-gaming.com/$1", "has": [{ "type": "host", "value": "www.xr-gaming.com" }], "permanent": true },
    { "source": "/(.*)", "destination": "https://xr-studios.com/$1", "has": [{ "type": "host", "value": "www.xr-studios.com" }], "permanent": true }
  ],
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml; charset=utf-8"
        }
      ]
    },
    {
      "source": "/robots.txt",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "/api/sitemap.xml.js" },
    { "source": "/(.*)", "destination": "/data-blast.com/$1", "has": [{ "type": "host", "value": "data-blast.com" }] },
    { "source": "/(.*)", "destination": "/gamer-guide.com/$1", "has": [{ "type": "host", "value": "gamer-guide.com" }] },
    { "source": "/(.*)", "destination": "/hair-guru.com/$1", "has": [{ "type": "host", "value": "hair-guru.com" }] },
    { "source": "/(.*)", "destination": "/vr-live.com/$1", "has": [{ "type": "host", "value": "vr-live.com" }] },
    { "source": "/(.*)", "destination": "/xr-gaming.com/$1", "has": [{ "type": "host", "value": "xr-gaming.com" }] },
    { "source": "/(.*)", "destination": "/xr-studios.com/$1", "has": [{ "type": "host", "value": "xr-studios.com" }] }
  ]
}
